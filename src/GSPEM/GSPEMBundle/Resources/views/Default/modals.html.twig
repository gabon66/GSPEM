{# app/Resources/views/base.html.twig #}
{% verbatim %}
<script type="text/ng-template" id="tareas_form.html">
    <div class="modal-header">
        <h3>Tarea</h3>
    </div>
    <div class="modal-body">
        <fieldset>



            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripci贸n(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <label>Tecnico :</label>

                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="tecnico.name for tecnico in tecnicos track by tecnico.id"

                 ng-model="tecnicotarea"></select>

            </div>

            <div class="form-group has-success has-feedback">
                <label>Sitio :</label>
                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="sitio.name for sitio in sitios track by sitio.id"

                 ng-model="sitiotarea"></select>
            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="save()">Guardar</a>
    </div>
</script>
<script type="text/ng-template" id="material_form.html">
    <div class="modal-header">
        <h3>Material</h3>
    </div>
    <div class="modal-body">
        <fieldset>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="id_custom"
                        class="form-control"
                        placeholder="ID">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripci贸n(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <label>Tipo :</label>

                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="type.name for type in types track by type.id"

                 ng-model="typematerial"></select>

            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveMaterial()">Guardar</a>
    </div>
</script>

<script type="text/ng-template" id="material_type_form.html">
    <div class="modal-header">
        <h3>Tipo de Material</h3>
