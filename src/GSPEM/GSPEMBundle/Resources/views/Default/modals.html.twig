{# app/Resources/views/base.html.twig #}
{% verbatim %}
<script type="text/ng-template" id="perfil_form.html">
    <div class="modal-header">
        <h3>Perfil</h3>
    </div>
    <div class="modal-body">
        <fieldset>
            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripción(*)">
            </div>
            <hr>

                       <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="perfiles_abm#home"><i class="fa fa-users"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu1"><i class="fa fa-map-marker"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu2"><i class="fa fa-home"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu3"><i class="fa fa-pencil"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu4"><i class="fa fa-car"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu5"><i class="fa fa-list-alt"></i></a></li>
                       </ul>

                       <div class="tab-content">
                         <div id="home" class="tab-pane fade in active">
                           <h3><input ng-model="perfil.user.all" type="checkbox" value=""> Usuarios</h3>
                            <div class="row">
                                <div class="col-lg-4"><input ng-model="perfil.user.abm" type="checkbox" value=""> ABM Usuarios</div>

                                <div class="col-lg-4"><input ng-model="perfil.user.contratistas" type="checkbox" value=""> ABM Contratistas</div>

                                <div class="col-lg-4"><input ng-model="perfil.user.perfiles" type="checkbox" value=""> ABM Perfiles</div>
                            </div>
                         </div>
                         <div id="menu1" class="tab-pane fade">
                           <h3><input ng-model="perfil.sitios.all" type="checkbox" value=""> Sitios</h3>
                           <div class="row">
                            <div class="col-lg-6"><input ng-model="perfil.sitios.abm"type="checkbox" value=""> ABM Sitios</div>
                           </div>
                         </div>
                         <div id="menu2" class="tab-pane fade">
                           <h3><input ng-model="perfil.materiales.all" type="checkbox" value=""> Materiales</h3>
                                <div class="row">
                                    <div class="col-lg-6"><input ng-model="perfil.materiales.abm_tipo" type="checkbox" value=""> ABM Tipo Materiales</div>
                                    <div class="col-lg-6"><input ng-model="perfil.materiales.abm" type="checkbox" value=""> ABM Materiales</div>
                                </div>
                         </div>
                          <div id="menu3" class="tab-pane fade">
                              <h3><input ng-model="perfil.oper.all" type="checkbox" value=""> Operaciones</h3>
                              <div class="row">
                                <div class="col-lg-4"><input ng-model="perfil.oper.stock" type="checkbox" value=""> Stock</div>
                                <div class="col-lg-4"><input ng-model="perfil.oper.stock_to_tec" type="checkbox" value=""> Maestro a Tecnico</div>
                                <div class="col-lg-4"><input ng-model="perfil.oper.stock_tec_to_tec" type="checkbox" value=""> Tecnico a Tecnico</div>
                              </div>

                            </div>
                              <div id="menu4" class="tab-pane fade">
                                <h3><input type="checkbox" value="" ng-model="perfil.opertec.all"> Operaciones Tecnico</h3>
                                    <div class="row">
                                     <div class="col-lg-2"><input ng-model="perfil.opertec.stock" type="checkbox" value=""> Stock</div>
                                     <div class="col-lg-3"><input ng-model="perfil.opertec.stoc_pend" type="checkbox" value=""> Stock Pendiente</div>
                                     <div class="col-lg-3"><input ng-model="perfil.opertec.stoc_tec" type="checkbox" value=""> Stock a Tecnico</div>
                                     <div class="col-lg-3"><input ng-model="perfil.opertec.stoc_sit" type="checkbox" value=""> Stock a Sitio</div>
                                    </div>
                                </div>
                              <div id="menu5" class="tab-pane fade">
                              <h3><input type="checkbox" value="" ng-model="perfil.reportes.all">  Reportes</h3>
                               <div class="row">
                                 <div class="col-lg-4"><input ng-model="perfil.reportes.stock_maestro" type="checkbox" value=""> Stock Maestro</div>
                                 <div class="col-lg-4"><input ng-model="perfil.reportes.stock_tec" type="checkbox" value=""> Stock por Tecnico</div>
                                 <div class="col-lg-4"><input ng-model="perfil.reportes.stock_sit" type="checkbox" value=""> Stock por Sitio</div>

                            </div>
                       </div>
            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveProfile()">Guardar</a>
    </div>
</script>

<script type="text/ng-template" id="tareas_form.html">
    <div class="modal-header">
        <h3>Tarea</h3>
    </div>
    <div class="modal-body">
        <fieldset>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripción(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <label>Tecnico :</label>

                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="tecnico.name for tecnico in tecnicos track by tecnico.id"

                 ng-model="tecnicotarea"></select>

            </div>

            <div class="form-group has-success has-feedback">
                <label>Sitio :</label>
                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="sitio.name for sitio in sitios track by sitio.id"

                 ng-model="sitiotarea"></select>
            </div>

        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="save()">Guardar</a>
    </div>
</script>
<script type="text/ng-template" id="material_form.html">
    <div class="modal-header">
        <h3>Material</h3>
    </div>
    <div class="modal-body">
        <fieldset>
            <div class="form-group has-success has-feedback">
                <input
                        ng-model="id_custom"
                        class="form-control"
                        placeholder="ID">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripción(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <label>Referencia Asignada :</label>

                <button class="btn btn-sm" style="background-color: {{ referencia.ref1 }}"></button>
                <button class="btn btn-sm" style="background-color: {{ referencia.ref2 }}"></button>
                <br/>
                    <label>Paleta de colores  :</label>
                  <div class="btn-group">
                      <a href class="btn btn-default" style="background-color: blue"  ng-click="setRef(1,'blue')" ui-exclusion-group='myButtons'></a>
                      <a href class="btn btn-default" style="background-color: green" ng-click="setRef(1,'green')" ui-exclusion-group='myButtons'></a>
                      <a href class="btn btn-default" style="background-color: black" ng-click="setRef(1,'black')" ui-exclusion-group='myButtons'></a>
                  </div>

                <div class="btn-group">
                      <a href class="btn btn-default" style="background-color: blue"  ng-click="setRef(2,'blue')" ui-exclusion-group='myButtons'></a>
                      <a href class="btn btn-default" style="background-color: green" ng-click="setRef(2,'green')" ui-exclusion-group='myButtons'></a>
                      <a href class="btn btn-default" style="background-color: black" ng-click="setRef(2,'black')" ui-exclusion-group='myButtons'></a>
                  </div>

            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="origen"
                        class="form-control"
                        placeholder="Origen(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="ubicacion"
                        class="form-control"
                        placeholder="Ubicación(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <label>Tipo :</label>

                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="type.name for type in types track by type.id"

                 ng-model="typematerial"></select>

            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveMaterial()">Guardar</a>
    </div>
</script>

