<script type="text/ng-template" id="sitio_form.html">
    <div class="modal-header">
        <h3>Sitio</h3>
    </div>
    <div class="modal-body">
        <fieldset>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripción">
            </div>

            <div class="form-group has-success has-feedback">
            <a ng-show="id!=0" ng-click="editing=true" class="btn btn-success btn-sm">Editar</a>
            <label ng-show="!editing" >{{ direccion_str  }}</label>
            <input ng-show="editing" type="text" ng-model="direccion" placeholder="Ingrese direccion aqui (*)" class="form-control" g-places-autocomplete ng-model="myScopeVar" />
              <!--  <input type="text"  id="Autocomplete" placeholder="Ingrese direccion aqui " class="form-control" ng-autocomplete="result1" ng-change="placeChanged()" on-place-changed="placeChanged(place)" details="details1" options="options1"/>-->
            </div>

        </fieldset>
    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveSitio()">Guardar</a>
    </div>
</script>


<script type="text/ng-template" id="newUser.html">
    <div class="modal-header">
        <h3>Usuario</h3>
    </div>

    <div class="modal-body">
    <fieldset>


<div class="form-inline">
    <div class="form-group">
     <label>Nombre(*)</label>
     <input
             ng-model="nombre"
             class="form-control"
             placeholder="Nombre">
    </div>

    <div class="form-group">
     <label>Apellido</label>
     <input
             ng-model="apellido"
             class="form-control"
             placeholder="Apellido">
    </div>
</div>
<hr>
            <div class="form-inline">
                <div class="form-group">
                <label>E-Mail(*)</label>
                <input
                        ng-model="mail"
                        class="form-control"
                        placeholder="eMail">
               </div>

               <div class="form-group">
                <label>Telefono</label>
                <input
                        ng-model="phone"
                        class="form-control"
                        placeholder="Telefono">
               </div>
            </div>

<hr>

    <div class="form-group has-success has-feedback">
     <label>Usuario(*)</label>
     <input
             ng-model="user"
             class="form-control"
             placeholder="Usuario">
    </div>

    <div class="form-group has-success has-feedback">
     <label>Clave(*)</label>
     <div class="form-inline">
       <a ng-click="editPass()" class="btn btn-warning btn-sm"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>

      <input
             type="password"
             ng-model="pass"
             class="form-control"
             placeholder="Clave" ng-disabled="passedite">
      </div>
    </div>
<hr>

            <div class="form-group has-success has-feedback">
                <label>Perfil :</label>

                <select  class="form-control" name="typematerial" id="mySelect"
                 ng-options="perfil.name for perfil in perfiles track by perfil.id"

                 ng-model="profileselected"></select>

            </div>

            <div class="form-group has-success has-feedback">
                <label><input ng-model="contratistaenabled" type="checkbox" value=""> Contratista :</label>

                <select ng-show="contratistaenabled==true"  class="form-control" name="typematerialss" id="mySelectContr"
                 ng-options="contratista.name for contratista in contratistas track by contratista.id"
                 ng-change="updateCon()"
                 ng-model="contratistaselected"></select>
                <label ng-if="contratistaenabled==false">Sin Asigar</label>

            </div>

        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveUser()">Guardar</a>
    </div>
</script>
 {% endverbatim %}